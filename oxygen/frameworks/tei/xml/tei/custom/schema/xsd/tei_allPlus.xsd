              <xs:choice>
                <xs:group ref="ns1:tei_castItem"/>
                <xs:group ref="ns1:tei_castGroup"/>
                <xs:group ref="ns1:tei_roleDesc"/>
              </xs:choice>
              <xs:group minOccurs="0" maxOccurs="unbounded" ref="ns1:tei_model.global"/>
            </xs:sequence>
