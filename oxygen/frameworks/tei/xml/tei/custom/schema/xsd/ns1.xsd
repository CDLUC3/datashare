    <xs:annotation>
      <xs:documentation>(geographic coordinates declaration) documents the notation and the datum used for geographic
  coordinates expressed as content of the geo element
  elsewhere within the document.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="teix:macro.phraseSeq">
          <xs:attributeGroup ref="teix:att.global.attribute.xmlid"/>
          <xs:attributeGroup ref="teix:att.global.attribute.n"/>
          <xs:attributeGroup ref="teix:att.global.attribute.xmllang"/>
