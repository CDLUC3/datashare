  <xs:attributeGroup name="att.editLike.attribute.cert">
    <xs:attribute name="cert">
      <xs:annotation>
        <xs:documentation>(certainty) signifies the degree of certainty associated with the
	intervention or interpretation.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
