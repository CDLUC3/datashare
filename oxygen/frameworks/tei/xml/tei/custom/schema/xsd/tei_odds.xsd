    <xs:sequence>
      <xs:element ref="ns1:seg"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="model.hiLike">
    <xs:sequence>
      <xs:element ref="ns1:hi"/>
    </xs:sequence>
  </xs:group>
