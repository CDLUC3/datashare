            <rng:ref name="rubric"/>
          </rng:optional>
          <rng:optional>
            <rng:ref name="incipit"/>
          </rng:optional>
          <rng:zeroOrMore>
            <rng:ref name="msItemStruct"/>
          </rng:zeroOrMore>
          <rng:optional>
            <rng:ref name="explicit"/>
          </rng:optional>
          <rng:optional>
            <rng:ref name="finalRubric"/>
          </rng:optional>
          <rng:zeroOrMore>
            <rng:ref name="colophon"/>
          </rng:zeroOrMore>
          <rng:zeroOrMore>
            <rng:ref name="decoNote"/>
          </rng:zeroOrMore>
          <rng:zeroOrMore>
            <rng:ref name="listBibl"/>
          </rng:zeroOrMore>
          <rng:zeroOrMore>
            <rng:ref name="bibl"/>
